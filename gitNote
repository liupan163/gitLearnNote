//已经建立的项目，关联到git
git init
//建立的同时，关联到git
git init "projectName"

git log
git log --all
git log --oneline  //简洁展示
git log --graph    //图形化

gitk

git branch -av

git cat-file -t    4235      //查看类型
git cat-file -p    4235      //查看内容

//HEAD本质就是个commit
git diff HEAD HEAD^1^1
git diff HEAD HEAD~2

//删除分支
git branch -d 98652532
git branch -D 89123216

git commit --amend